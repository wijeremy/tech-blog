{{#each posts as |post|}}
  <div class='card'>
    <div class='card-body row'>
      <h5 class='card-title col-12 col-sm-3'>{{post.title}}</h5>
      <p class='col-12 col-sm-3'>by {{post.name}}</p>
      <p class='col-12 col-sm-3'>on {{post.createdAt}} </p>
    </div>
  </div>
{{/each}}

